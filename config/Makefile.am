pkgsysconfdir=$(sysconfdir)/@PACKAGE@
pkgsysconf_DATA = \
	SECURACTIVE-MIB.txt \
	JUNKIE-MIB.txt \
	instvars.scm \
	defs.scm \
	snmp-subagt.scm \
	start-snmp.scm \
	start-repl.scm \
	junkie.rlwrap

EXTRA_DIST = $(pkgsysconf_DATA)

instvars.scm:
	@echo ";;; This file is autogenerated by Makefile. Do not edit it" > $@
	@echo "" >> $@
	@echo '(define bindir "$(bindir)")' >> $@
	@echo '(define libdir "$(libdir)")' >> $@
	@echo '(define confdir "$(pkgsysconfdir)")' >> $@

